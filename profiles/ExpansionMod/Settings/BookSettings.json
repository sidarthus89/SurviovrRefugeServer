    "RuleCategories": [
        {
            "CategoryName": "General Rules",
            "Rules": [
                {
                    "RuleParagraph": "1.1.",
                    "RuleText": "No Glitching, Exploiting, Loot Cycling, Duping or Hacking "
                },
                {
                    "RuleParagraph": "1.2.",
                    "RuleText": "No Combat Logging (You Must wait at least 10 Minutes after interacting/fighting with another player before logging out)"
                },
                {
                    "RuleParagraph": "1.3.",
                    "RuleText": "No Meta Gaming(don't call out: player locations[including your own], in game events[like heli crashes], base locations in the global chat)"
                },
                {
                    "RuleParagraph": "1.4.",
                    "RuleText": "No Stream Sniping or Stalking"
                },
                {
                    "RuleParagraph": "1.5.",
                    "RuleText": "If you suspect someone of cheating, put in a ticket."
                }
            ]
        },
		{
            "CategoryName": "Trader Rules",
            "Rules": [
                {
                    "RuleParagraph": "1.1.",
                    "RuleText": "Do not steal inside of traders"
                },
                {
                    "RuleParagraph": "1.2.",
                    "RuleText": "Do not follow people out of traders to kill them."
                },
                {
                    "RuleParagraph": "1.3.",
                    "RuleText": "Do not camp any traders (Yes this includes BM)."
                },
                {
                    "RuleParagraph": "1.4.",
                    "RuleText": "Provocations and toxic behavior will not be tolerated and punished! Be friendly to fellow players and your team, both in chat and in voice!"
                },
                {
                    "RuleParagraph": "1.5.",
                    "RuleText": "Safezones/traders are not for keeping your vehicles at. If a vehicle is abandoned at a trader/safezone (ie: no one is on the server, or no one claims it as theirs) it will be removed and no refund will be given.."
                },
                {
                    "RuleParagraph": "1.6.",
                    "RuleText": "Trolling other players in traders will be considered as toxic behavior"
                },
                {
                    "RuleParagraph": "1.7.",
                    "RuleText": "Do not run to a safe zone to escape from PVP."
                },
                {
                    "RuleParagraph": "1.8.",
                    "RuleText": "Do not loiter / AFK in the safe zone for long periods of time, you will be kicked."
                },
                {
                    "RuleParagraph": "1.9.",
                    "RuleText": "No stashes, safes or containers inside the safezone, these will be removed"
                },
                {
                    "RuleParagraph": "1.10.",
                    "RuleText": "Clean up after yourself. Use the donation barrel and garbage can. Items left on the ground for longer than 30 minutes risk being deleted.."
                },
                {
                    "RuleParagraph": "1.11.",
                    "RuleText": "Do Not Call Out Trader Campers In Side Chat, Report Them in the #deleted-channel channel."
                }
            ]
        },
		{
            "CategoryName": "Basebuilding Rules",
            "Rules": [
                {
                    "RuleParagraph": "1.1.",
                    "RuleText": "1 flag per group/solo."
                },
                {
                    "RuleParagraph": "1.2.",
                    "RuleText": "No floating/physics defying bases."
                },
                {
                    "RuleParagraph": "1.3.",
                    "RuleText": "Territory sizes limited to 40m."
                },
                {
                    "RuleParagraph": "1.4.",
                    "RuleText": "No building within 850m of Coast, 1km of Large Military, 500m of Small Military (2-3 tents/bldgs) & 1km of Traders"
                },
                {
                    "RuleParagraph": "1.5.",
                    "RuleText": "No hoarding helis. Helis should reflect your group size not your ego - If you're in a six man group go look for a Huey or two Little Birds etc."
                },
				{
                    "RuleParagraph": "1.6.",
                    "RuleText": "Max three vehicles per base"
                },
                {
                    "RuleParagraph": "1.7.",
                    "RuleText": "Do not build on main roads."
                },
                {
                    "RuleParagraph": "1.8.",
                    "RuleText": "Do not deploy flags without the intention of building bases around or with them, a solo flag will be deleted if there hasn't been an attempt to build around it."
                },
                {
                    "RuleParagraph": "1.9.",
                    "RuleText": "Camping a base for an extended period of time (ie: 5 hours a day 5 days a week. Not 30 minutes) will be considered as toxic behavior "
                },
                {
                    "RuleParagraph": "1.10.",
                    "RuleText": "Raiding on this server is done by using high yield explosives. Finding and using exploits to remove walls by non approved means is considered abusing an exploit and you will be banned."
                }
            ]
        }
    ],
